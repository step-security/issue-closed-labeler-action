{
  "name": "issue-closed-labeler-action",
  "version": "1.0.2",
  "private": true,
  "description": "Conditionally add labels to issues when closed.",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "homepage": "https://github.com/step-security/issue-closed-labeler-action#readme",
  "bugs": {
    "url": "https://github.com/step-security/issue-closed-labeler-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/step-security/issue-closed-labeler-action.git"
  },
  "license": "BSD-3-Clause",
  "author": "step-security",
  "type": "module",
  "module": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.mjs",
    "check-format": "prettier --list-different \"./**/*.{md,ts,yml}\"",
    "check-spelling": "cspell --config=.cspell.json \"**/*.{md,ts}\"",
    "format": "prettier --write \"./**/*.{md,ts,yml}\"",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint .",
    "lint:md": "markdownlint \"**/*.md\" --config=.markdownlint.json --ignore-path=.markdownlintignore",
    "test": "c8 ava"
  },
  "dependencies": {
    "@actions/core": "2.0.1",
    "@actions/github": "6.0.1",
    "@octokit/graphql": "7.1.1",
    "@octokit/types": "^7.2.0",
    "ava": "5.3.1",
    "axios": "^1.13.4",
    "eslint-plugin-unicorn": "53.0.0"
  },
  "devDependencies": {
    "@rebeccastevens/eslint-config": "^1.2.2",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/node": "^18.0.0",
    "@types/rollup-plugin-auto-external": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.30.0",
    "c8": "^7.11.3",
    "cspell": "^6.8.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^62.6.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "markdownlint-cli": "^0.47.0",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "ts-node": "^10.9.1",
    "ts-paths-esm-loader": "^1.3.0",
    "tslib": "^2.4.0",
    "type-fest": "^3.0.0",
    "typescript": "^4.7.2"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=20.19.1"
  }
}
